{
	"extends": [
		"tslint:latest",
		"tslint-react"
	],
	"rules": {
		"max-line-length": [
			true,
			120
		],
		"quotemark": [
			true,
			"single"
		],
		"indent": [
			true,
			"tabs"
		],
		"variable-name": [
			true, "allow-leading-underscore"
		],
		"ordered-imports": [
			false
		]
	},
	"defaultSeverity": "warning"
}
