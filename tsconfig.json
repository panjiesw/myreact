{
	"compilerOptions": {
		"target": "es5",
		"module": "es6",
		"strictNullChecks": true,
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"charset": "utf8",
		"jsx": "react",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "dist",
		"pretty": true,
		"baseUrl": "."
	},
	"exclude": [
		"dist",
		"node_modules"
	],
	"compileOnSave": false
}
