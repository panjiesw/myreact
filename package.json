{
	"name": "myreact",
	"version": "1.0.0",
	"description": "My React Playground",
	"main": "index.js",
	"repository": "https://github.com/panjiesw/myreact.git",
	"author": {
		"name": "Panjie Setiawan Wicaksono",
		"email": "<panjie@panjiesw.com>",
		"url": "https://panjiesw.life"
	},
	"license": "MIT",
	"scripts": {
		"build": "webpack --env.production --progress --profile",
		"clean": "npm run clean:cache && npm run clean:dist",
		"clean:cache": "del .awcache",
		"clean:dist": "del dist",
		"lint": "npm run lint:ts && npm run lint:css",
		"lint:css": "stylelint ./src/**/*.scss",
		"lint:ts": "tslint -e './node_modules/**/*' -e './dist/**/*' -e './**/*.d.ts' './**/*.{ts,tsx}'",
		"start": "cross-env UV_THREADPOOL_SIZE=100 webpack-dev-server",
		"sdts": "sdts -p './src/**/theme.scss'",
		"prebuild": "npm run lint && npm run clean",
		"prebuild:fb": "npm run lint && npm run clean",
		"prestart": "npm run lint && npm run clean:cache"
	},
	"devDependencies": {
		"@types/classnames": "^0.0.32",
		"@types/node": "^7.0.5",
		"@types/react": "^15.0.6",
		"@types/react-dom": "^0.14.22",
		"@types/react-hot-loader": "^3.0.1",
		"@types/react-router": "^2.0.45",
		"@types/scriptjs": "^0.0.2",
		"@types/whatwg-fetch": "^0.0.33",
		"autoprefixer": "^6.7.0",
		"awesome-typescript-loader": "^3.0.3",
		"compression-webpack-plugin": "^0.3.2",
		"cross-env": "^3.1.4",
		"css-loader": "^0.26.1",
		"del-cli": "^0.2.1",
		"dotenv-safe": "^4.0.3",
		"extract-text-webpack-plugin": "^2.0.0-rc.3",
		"favicons": "^4.8.3",
		"file-loader": "^0.9.0",
		"firebase": "^3.6.6",
		"firebaseui": "^1.0.0",
		"flexbox-react": "^4.2.1",
		"html-webpack-plugin": "^2.26.0",
		"loader-utils": "^0.2.16",
		"node-sass": "^4.3.0",
		"null-loader": "^0.1.1",
		"postcss-loader": "^1.2.2",
		"react-hot-loader": "^3.0.0-beta.6",
		"sass-loader": "^4.1.1",
		"sass-to-dts": "^0.2.2",
		"style-loader": "^0.13.1",
		"stylelint": "^7.7.1",
		"stylelint-config-standard": "^15.0.1",
		"stylelint-webpack-plugin": "^0.5.1",
		"tslint": "^4.3.1",
		"typescript": "^2.1.5",
		"webpack": "^2.2.1",
		"webpack-configs": "^0.1.3",
		"webpack-dev-server": "^2.3.0",
		"webpack-merge": "^2.6.1"
	},
	"dependencies": {
		"classnames": "^2.2.5",
		"frest": "^0.4.0",
		"frest-interceptor-json": "^0.1.0",
		"mobx": "^3.1.0",
		"mobx-react": "^4.1.0",
		"mobx-utils": "^2.0.1",
		"react": "^15.4.2",
		"react-addons-css-transition-group": "^15.4.2",
		"react-addons-transition-group": "^15.4.2",
		"react-dom": "^15.4.2",
		"react-md": "^1.0.2",
		"react-router": "^3.0.2",
		"rxjs": "^5.0.3",
		"scriptjs": "^2.5.8",
		"tslib": "^1.5.0"
	},
	"stylelint": {
		"extends": "stylelint-config-standard",
		"rules": {
			"indentation": "tab",
			"selector-pseudo-class-no-unknown": null
		},
		"ignoreFiles": [
			"node_modules",
			"dist"
		]
	},
	"browserslist": [
		"> 1%",
		"last 4 versions"
	]
}
