{
	"name": "myreact",
	"version": "1.0.0",
	"description": "My React Playground",
	"main": "index.js",
	"repository": "https://github.com/panjiesw/myreact.git",
	"author": "Panjie Setiawan Wicaksono <panjie@panjiesw.com>",
	"license": "MIT",
	"scripts": {
		"build": "webpack --env.prod --progress --profile",
		"build:fb": "webpack --env.prod --env.fb --progress --profile",
		"clean": "npm run clean:cache && npm run clean:dist",
		"clean:cache": "del-cli .awcache",
		"clean:dist": "del-cli dist",
		"nano": "cssnano dist/assets/globals.css dist/assets/globals.css && cssnano dist/assets/globals.css dist/assets/globals.css",
		"lint": "npm run lint:ts && npm run lint:css",
		"lint:css": "stylelint ./src/**/*.css",
		"lint:ts": "tslint -e './node_modules/**/*' -e './dist/**/*' -e './**/*.d.ts' './**/*.{ts,tsx}'",
		"start": "cross-env UV_THREADPOOL_SIZE=100 webpack-dev-server",
		"tcm": "tcm -p './src/**/theme.css'",
		"prebuild": "npm run lint && npm run clean",
		"prebuild:fb": "npm run lint && npm run clean",
		"prestart": "npm run lint && npm run clean:cache"
	},
	"devDependencies": {
		"@types/classnames": "^0.0.32",
		"@types/node": "^6.0.54",
		"@types/react": "^15.0.0",
		"@types/react-dom": "^0.14.20",
		"@types/react-router": "^2.0.42",
		"@types/scriptjs": "^0.0.2",
		"@types/systemjs": "^0.19.33",
		"@types/webpack": "^2.1.0",
		"awesome-typescript-loader": "^3.0.0-beta.17",
		"compression-webpack-plugin": "^0.3.2",
		"cross-env": "^3.1.3",
		"css-loader": "^0.26.1",
		"del-cli": "^0.2.1",
		"dotenv-safe": "^3.0.0",
		"extract-text-webpack-plugin": "beta",
		"favicons": "^4.8.3",
		"file-loader": "^0.9.0",
		"firebase": "^3.6.4",
		"firebaseui": "^1.0.0",
		"flexboxgrid": "^6.3.1",
		"html-webpack-plugin": "^2.24.1",
		"loader-utils": "^0.2.16",
		"null-loader": "^0.1.1",
		"postcss-cssnext": "^2.9.0",
		"postcss-each": "^0.9.3",
